model PlaceWorker {
  place   Place           @relation(fields: [placeId], references: [id], onDelete: Cascade)
  user    User            @relation(fields: [userId], references: [id], onDelete: Cascade)
  userId  Int
  placeId Int
  role    PlaceWorkerRole

  @@id([userId, placeId])
}

enum PlaceWorkerRole {
  ADMIN
  WAITER
}
