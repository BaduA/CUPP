model EarnedPlacePoint {
    id              Int           @id @default(autoincrement())
    earnedPoint     Int
    earnedMenuItem  PlaceMenuItem @relation(fields: [earnedMenuItemId], references: [id])
    earnedMenuItemId Int
    userRecord      PlaceUserRecord @relation(fields: [userRecordId], references: [id])
    userRecordId    Int
    @@map("earnedPlacePoints")
}
