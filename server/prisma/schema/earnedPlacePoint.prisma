model EarnedPlacePoint {
    id               Int             @id @default(autoincrement())
    earnedPoint      Int
    earnedPointMenuItems EarnedPointMenuItem[]
    userRecord       PlaceUserRecord @relation(fields: [userRecordId], references: [id])
    userRecordId     Int
    createdAt        DateTime        @default(now())
    @@map("earnedPlacePoints")
}
