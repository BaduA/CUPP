model PlacePromotion {
    id           Int             @id @default(autoincrement())
    name         String
    imageAddress String
    place        Place           @relation(fields: [placeId], references: [id])
    placeId      Int
    pointValue   Int
    userRecord   PlaceUserRecord @relation(fields: [userRecordId], references: [id])
    userRecordId Int
    @@map("placePromotions")
}
