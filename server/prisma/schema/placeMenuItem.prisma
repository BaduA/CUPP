model PlaceMenuItem {
    id                   Int                   @id @default(autoincrement())
    name                 String
    imageAddress         String?
    price                Float
    pointValue           Float
    size                 String
    place                Place                 @relation(fields: [placeId], references: [id], onDelete: Cascade)
    placeId              Int
    earnedPointMenuItems EarnedPointMenuItem[]

    @@map("placeMenuItems")
}
